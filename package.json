{
  "name": "music-app",
  "version": "1.0.0",
  "description": "To be updated",
  "main": "main.js",
  "scripts": {
    "dev:start": "npm-run-all -p dev:react dev:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.config.js --mode development && electron ./dist/electron.js",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --mode development --inline --hot",
    "dev:serve": "wait-on http-get://localhost:8080/ && npm run dev:electron",
    "build": "cross-env NODE_ENV=production && webpack --config ./webpack.config.js",
    "start": "npm run build && electron ./dist/electron.js",
    "postinstall": "install-app-deps",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts,.tsx --fix src/**",
    "format": "prettier --write src",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "author": "Evaldas Latoskinas",
  "license": "UNLICENSED",
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@jest-runner/electron": "^2.0.3",
    "@testing-library/jest-dom": "^5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/async": "^3.2.5",
    "@types/better-sqlite3": "^5.4.1",
    "@types/howler": "^2.1.2",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.166",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/yallist": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.4.2",
    "electron": "^8.1.1",
    "electron-builder": "^24.6.3",
    "electron-rebuild": "^3.2.9",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.19.0",
    "fast-glob": "^3.2.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.1",
    "jest-mock": "^29.6.2",
    "music-metadata": "^6.3.7",
